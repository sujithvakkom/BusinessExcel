@using BusinessExcel.Models
@using BusinessExcel.Providers.ProviderContext.Entities
@using BusinessExcel.Providers.ProviderContext
@using BusinessExcel.Controllers.JSON
@using BusinessExcel.Controllers
@using BootstrapHtmlHelper

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        @TargetController.TARGET
        <small>@TargetController.TARGETTEMPLATE_TITLE</small>
    </h1>
    <ol class="breadcrumb">
        <li>
            <a href="#"><i class="fa fa-dashboard"></i>@TargetController.LOCATIONTARGET_TITLE</a>
        </li>
        <li class="active">@AccountsController.WELCOME_TITLE</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12 col-md-6">
            @Html.Partial(TargetController._IMPORTDATA)
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            @Html.Partial("_BackupTarget", new BackupTargetFilter() { })
        </div>
    </div>
</section>