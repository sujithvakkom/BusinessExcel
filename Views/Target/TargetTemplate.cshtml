@using BusinessExcel.Models
@using BusinessExcel.Providers.ProviderContext.Entities
@using BusinessExcel.Providers.ProviderContext
@using BusinessExcel.Controllers.JSON
@using BusinessExcel.Controllers
@using BootstrapHtmlHelper

@model BaseTarget

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        @TargetController.TARGET
        <small>@TargetController.TARGETTEMPLATE_TITLE</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i>@TargetController.TARGETTEMPLATE_TITLE</a></li>
        <li class="active">@AccountsController.WELCOME_TITLE</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row" style="margin:10px">
        <div class="col-md-6">
            <div id="target-tamplet">
                @Html.Partial(TargetController._TARGETTEMPLATECREATEBLOCK, Model)
            </div>
        </div>
        <div class="col-md-6">
            <div id="target-assign">
                @Html.Partial(TargetController._TARGETASSIGNBLOCK, Model)
            </div>
        </div>
    </div>
</section>